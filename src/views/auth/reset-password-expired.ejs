<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../layouts/head', { title: 'Reset Link Expired - helloRun' }) %>
</head>
<body>
  <%- include('../layouts/nav') %>
  
  <main class="auth-page">
    <div class="auth-container">
      <div class="auth-card">
        <div class="auth-icon error">
          <i data-lucide="clock"></i>
        </div>
        <h1>Reset Link Expired</h1>
        <p class="auth-subtitle">This password reset link has expired. Please request a new one.</p>
        
        <div class="auth-actions">
          <a href="/forgot-password" class="btn btn-full">Request New Link</a>
          <a href="/login" class="btn btn-secondary btn-full">Back to Login</a>
        </div>
      </div>
    </div>
  </main>
  
  <%- include('../layouts/footer') %>
  
  <script src="/js/main.js"></script>
  <script>
    lucide.createIcons();
  </script>
</body>
</html>